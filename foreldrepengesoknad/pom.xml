    <artifactId>foreldrepengesoknad</artifactId>
    <packaging>jar</packaging>
    <name>Dokumentgenerator :: Foreldrepengesoknad</name>


    <dependencies>
        <!-- EngangsstÃ¸nad -og foreldrepenger-skjema for Utbyggeren -->
        <dependency>
            <groupId>no.nav.foreldrepenger.kontrakter</groupId>
            <artifactId>soeknad-v3</artifactId>
        </dependency>
        <dependency>
            <groupId>jakarta.validation</groupId>
            <artifactId>jakarta.validation-api</artifactId>
        </dependency>
        <dependency>
            <groupId>com.neovisionaries</groupId>
            <artifactId>nv-i18n</artifactId>
